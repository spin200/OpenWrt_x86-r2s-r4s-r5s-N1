# 启用ramips架构支持  
CONFIG_TARGET_ramips=y  
  
# 启用ramips架构下的mt7621芯片支持  
CONFIG_TARGET_ramips_mt7621=y  
  
# 为mt7621芯片设定设备为hiwifi_hc5962  
CONFIG_TARGET_ramips_mt7621_DEVICE_hiwifi_hc5962=y  
  
# 启用6in4软件包支持（用于IPv6过渡机制）  
CONFIG_PACKAGE_6in4=y  
  
# 设置SAMBA的最大调试级别为-1（关闭调试信息）  
CONFIG_PACKAGE_SAMBA_MAX_DEBUG_LEVEL=-1  
  
# 启用自动挂载软件包支持  
CONFIG_PACKAGE_automount=y  
  
# 启用autosamba软件包支持（自动配置SAMBA共享）  
CONFIG_PACKAGE_autosamba=y  
  
# 启用cfdisk软件包支持（用于磁盘分区）  
CONFIG_PACKAGE_cfdisk=y  
  
# 启用fdisk软件包支持（用于磁盘分区）  
CONFIG_PACKAGE_fdisk=y  
  
# 启用frpc软件包支持（用于实现远程过程调用）  
CONFIG_PACKAGE_frpc=y  
  
# 启用hd-idle软件包支持（硬盘空闲管理）  
CONFIG_PACKAGE_hd-idle=y  
  
# 启用ip6tables软件包支持（IPv6防火墙管理）  
CONFIG_PACKAGE_ip6tables=y  
  
# 启用iptables-mod-ipopt软件包支持（用于IP选项的iptables模块）  
CONFIG_PACKAGE_iptables-mod-ipopt=y  
  
# 启用iputils-arping软件包支持（用于发送ARP请求）  
CONFIG_PACKAGE_iputils-arping=y  
  
# 启用ipv6helper软件包支持（IPv6辅助工具）  
CONFIG_PACKAGE_ipv6helper=y  
  
# 启用jq软件包支持（强大的JSON处理工具）  
CONFIG_PACKAGE_jq=y  
  
# 启用kcptun-client软件包支持（KCP协议的客户端）  
CONFIG_PACKAGE_kcptun-client=y  
  
# 启用kcptun-config软件包支持（KCP协议的配置工具）  
CONFIG_PACKAGE_kcptun-config=y  
  
# 启用kmod-crypto-crc32c模块支持（用于CRC32C校验）  
CONFIG_PACKAGE_kmod-crypto-crc32c=y  
  
# 以下几行为文件系统模块支持，包括exFAT、ext4、ntfs3-oot、vfat等  
CONFIG_PACKAGE_kmod-fs-exfat=y       # exFAT文件系统模块支持  
CONFIG_PACKAGE_kmod-fs-ext4=y        # ext4文件系统模块支持  
CONFIG_PACKAGE_kmod-fs-ntfs3-oot=y   # ntfs3-oot文件系统模块支持  
CONFIG_PACKAGE_kmod-fs-vfat=y        # vfat文件系统模块支持  
  
# 以下几行为网络模块相关，包括iptunnel、nat6、iptunnel4等网络模块的支持  
CONFIG_PACKAGE_kmod-ipt-ipopt=y  
CONFIG_PACKAGE_kmod-ipt-nat6=y  
CONFIG_PACKAGE_kmod-iptunnel=y  
CONFIG_PACKAGE_kmod-iptunnel4=y  
CONFIG_PACKAGE_kmod-nf-nat6=y  
  
# 以下为其他模块和库的支持，如crc16库、SCSI核心模块、USB存储模块等  
CONFIG_PACKAGE_kmod-lib-crc16=y         # crc16库支持  
CONFIG_PACKAGE_kmod-nls-cp437=y         # cp437字符集支持  
CONFIG_PACKAGE_kmod-nls-iso8859-1=y     # iso8859-1字符集支持  
CONFIG_PACKAGE_kmod-nls-utf8=y          # utf8字符集支持  
CONFIG_PACKAGE_kmod-scsi-core=y         # SCSI核心模块支持  
CONFIG_PACKAGE_kmod-sit=y               # sit模块支持  
CONFIG_PACKAGE_kmod-usb-storage=y       # USB存储模块支持  
CONFIG_PACKAGE_kmod-usb-storage-extras=y # USB存储扩展模块支持  
# 以下为一些库的支持，如libatomic、libblkid、libevent2等  
CONFIG_PACKAGE_libatomic=y              # 原子操作库支持
# 启用libblkid库支持，用于设备块识别  
CONFIG_PACKAGE_libblkid=y  
  
# 启用libevent2库支持，用于事件驱动的网络编程  
CONFIG_PACKAGE_libevent2=y  
  
# 启用libfdisk库支持，用于磁盘分区操作  
CONFIG_PACKAGE_libfdisk=y  
  
# 启用libmount库支持，用于挂载文件系统  
CONFIG_PACKAGE_libmount=y  
  
# 启用libsmartcols库支持，用于终端列表显示  
CONFIG_PACKAGE_libsmartcols=y  
  
# 启用libwebsockets-full库支持，用于构建WebSocket全功能应用  
CONFIG_PACKAGE_libwebsockets-full=y  
  
# 启用LuCI的Argon配置应用支持  
CONFIG_PACKAGE_luci-app-argon-config=y  
  
# 启用LuCI的frpc应用支持  
CONFIG_PACKAGE_luci-app-frpc=y  
  
# 启用LuCI的guest-wifi应用支持，用于访客WiFi管理  
CONFIG_PACKAGE_luci-app-guest-wifi=y  
  
# 启用LuCI的hd-idle应用支持，用于硬盘空闲管理  
CONFIG_PACKAGE_luci-app-hd-idle=y  
  
# 启用LuCI的mwan3应用支持，用于多WAN口管理  
CONFIG_PACKAGE_luci-app-mwan3=y  
  
# 启用LuCI的pushbot应用支持，用于推送通知  
CONFIG_PACKAGE_luci-app-pushbot=y  
  
# 启用LuCI的samba应用支持，用于Samba文件共享管理  
CONFIG_PACKAGE_luci-app-samba=y  
# 启用LuCI的adguardhome应用支持，用于DNS管理过滤广告
CONFIG_PACKAGE_luci-app-adguardhome=y
CONFIG_PACKAGE_adguardhome=y
  
# 以下几项为LuCI的ssr-plus应用的不同模块支持  
CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_IPT2Socks=y   # 支持IPT2Socks模块  
CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_Kcptun=y      # 支持Kcptun模块  
CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_NaiveProxy=y  # 支持NaiveProxy模块  
CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_Redsocks2=y   # 支持Redsocks2模块  
CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_Xray=y        # 支持Xray模块  
  
# 启用LuCI的syncdial应用支持，用于同步拨号管理  
CONFIG_PACKAGE_luci-app-syncdial=y  
  
# 启用LuCI的ttyd应用支持，用于终端分享和访问  
CONFIG_PACKAGE_luci-app-ttyd=y  
  
# 启用LuCI的兼容性支持  
CONFIG_PACKAGE_luci-compat=y  
  
# 以下几项为LuCI应用的中文（简体）语言包支持  
CONFIG_PACKAGE_luci-i18n-argon-config-zh-cn=y  
CONFIG_PACKAGE_luci-i18n-frpc-zh-cn=y  
CONFIG_PACKAGE_luci-i18n-guest-wifi-zh-cn=y  
CONFIG_PACKAGE_luci-i18n-hd-idle-zh-cn=y  
CONFIG_PACKAGE_luci-i18n-mwan3-zh-cn=y  
CONFIG_PACKAGE_luci-i18n-samba-zh-cn=y  
CONFIG_PACKAGE_luci-i18n-ttyd-zh-cn=y  
  
# 启用LuCI的IPv6协议支持  
CONFIG_PACKAGE_luci-proto-ipv6=y  
  
# 启用LuCI的ifit主题支持  
CONFIG_PACKAGE_luci-theme-ifit=y  
  
# 启用LuCI的neobird主题支持  
CONFIG_PACKAGE_luci-theme-neobird=y  
  
# 启用mwan3库支持，实现多WAN口功能的核心库  
CONFIG_PACKAGE_mwan3=y  
  
# 启用naiveproxy支持，一个轻量级的网络代理工具  
CONFIG_PACKAGE_naiveproxy=y  
  
# 启用nano文本编辑器支持  
CONFIG_PACKAGE_nano=y  
# 启用ntfs3文件系统挂载支持  
CONFIG_PACKAGE_ntfs3-mount=y
# 启用odhcp6c支持。odhcp6c是一个用于IPv6的网络协议DHCP（动态主机配置协议）客户端。  
CONFIG_PACKAGE_odhcp6c=y  
  
# 设置odhcp6c的扩展证书ID为0。这个参数一般用于指定证书ID，如果不需要特别的证书，就会设为0。  
CONFIG_PACKAGE_odhcp6c_ext_cer_id=0  
  
# 启用odhcpd的IPv6 only模式。odhcpd是一个简单的DHCP服务器，IPv6 only模式意味着它只支持IPv6协议。  
CONFIG_PACKAGE_odhcpd-ipv6only=y  
  
# 设置odhcpd的IPv6 only模式的扩展证书ID为0。同样的，这个参数用于指定证书ID，不需要特别证书时设为0。  
CONFIG_PACKAGE_odhcpd_ipv6only_ext_cer_id=0  
  
# 启用redsocks2支持。redsocks2是一个用于实现SOCKS代理协议的工具，它可以和其他工具一起使用以增加网络连接的匿名性和安全性。  
CONFIG_PACKAGE_redsocks2=y  
  
# 启用samba36-server支持。Samba是一种开源软件，用于Linux和Unix系统实现SMB/CIFS网络协议，使得这些系统能够和Windows系统共享文件和打印机。  
CONFIG_PACKAGE_samba36-server=y  
  
# 启用ttyd支持。ttyd是一个简单的命令行工具，它可以共享终端会话，使得其他用户可以通过Web浏览器访问这个会话。  
CONFIG_PACKAGE_ttyd=y  
  
# 启用wsdd2支持。wsdd2是一个用于实现Web服务的Discovery和Description的工具，它使得设备可以在网络中自动发现和配置。  
CONFIG_PACKAGE_wsdd2=y  
  
# 启用xray-core支持。xray是一个开源的网络代理工具，它支持多种协议，并提供了强大的路由、代理和其他网络功能。  
CONFIG_PACKAGE_xray-core=y
